{
    "DataStores": {
        "My_Minio_Bucket": {
            "Endpoint": "http://play.min.io",
            "Bucket": "faasr",
            "Writable": "TRUE"
        }
    },
    "ComputeServers": {
        "My_GitHub_Account": {
            "FaaSType": "GitHubActions",
            "UserName": "Ashish-Ramrakhiani",
            "ActionRepoName": "Faasr_single_function_action",
            "Branch": "main"
        }
    },
    "FunctionList": {
        "Action1": {
            "FunctionName": "create_sample_data",
            "FaaSServer": "My_GitHub_Account",
            "Arguments": {
                "folder": "myexample",
                "output1": "sample1.csv",
                "output2": "sample2.csv"
            },
            "InvokeNext": "Sum"
        },
        "Sum": {
            "FunctionName": "compute_sum",
            "FaaSServer": "My_GitHub_Account",
            "Arguments": {
                "folder": "myexample",
                "input1": "sample1.csv",
                "input2": "sample2.csv",
                "output": "sum.csv"
            }
        }
    },
    "FunctionGitRepo": {
        "create_sample_data": "Ashish-Ramrakhiani/Faasr_single_function_code",
        "compute_sum": "Ashish-Ramrakhiani/Faasr_single_function_code"
    },
    "FunctionInvoke": "Action1",
    "FaaSrLog": "FaaSrLog",
    "LoggingDataStore": "My_Minio_Bucket",
    "DefaultDataStore": "My_Minio_Bucket"
}

